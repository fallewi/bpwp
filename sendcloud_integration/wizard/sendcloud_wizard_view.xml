<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="sendcloud_wizard_form" model="ir.ui.view">
		<field name="name">sendcloud.wizard</field>
		<field name="model">sendcloud.wizard</field>
		<field name="arch" type="xml">
			<form string="Message">
				<separator string="Message" colspan="6" />
				<field name="text" colspan="4" nolabel="1" />
				<footer>
					<button special="cancel" class="oe_highlight"
						string="Close" />
				</footer>
			</form>
		</field>
	</record>
</odoo>
